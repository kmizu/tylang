// TyLang sample program
fun add(x: Int, y: Int): Int {
  x + y
}

fun factorial(n: Int): Int {
  if (n <= 1) {
    1
  } else {
    n * factorial(n - 1)
  }
}

class Point(x: Int, y: Int) {
  fun getX(): Int { x }
  fun getY(): Int { y }
  fun distance(): Double {
    0.0
  }
}

object Math {
  fun pi(): Double { 3.14159 }
  fun square(x: Int): Int { x * x }
}